{% extends "layout.njk" %}

{% block content %}

<div id="wrapper">
          <img src="{{ data.logo_url }}" alt="imagem de {{ data.name }}">
          <h1> {{ data.name }} </h1>
          <h2> {{ data.welcome }} </h2>
          <p> {{ data.description }} </p>
       </div>

        
        
       <div id="links-footer" class="links">
           {% for link in data.links %}
           
           <a href="{{ link.url }}" target="_blank">{{ link.name }}</a>

           {% endfor %}

        </div>


{% endblock content %}
